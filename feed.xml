---
layout: rss
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
	<channel>
		<title>Tyler Tate</title>
		<description>Making the complex feel simple.</description>
		<link>http://www.tylertate.com/</link>
		{% for post in site.posts %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				<guid>{{ post.url }}</guid>
				<pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
				<description>{{ post.content | xml_escape }}</description>
				<link>http://www.tylertate.com{{ post.url }}</link>
			</item>
		{% endfor %}
	</channel>
</rss>